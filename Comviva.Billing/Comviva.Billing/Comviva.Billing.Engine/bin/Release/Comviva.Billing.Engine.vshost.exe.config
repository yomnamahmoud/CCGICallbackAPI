<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  <appSettings>
    <add key="WaitHandle" value="1000" />
    <add key="ApplicationName" value="Comviva billing engine"/>
    <add key="CustomLoggerLogsFile" value="CustomLogs"/>
    <add key="LogAllCustomLogs" value="true"/>
    <add key="LogSqlCommands" value="false"/>
    <add key="PayloadTemplatePath" value="PayloadTemplate.xml" />
  </appSettings>
  <connectionStrings>
    <add name="DefaultConnectionString" connectionString="data source=.;initial catalog=MazadyComviva;integrated security=true" />
  </connectionStrings>
</configuration>